{
    "orgName": "omni-cci - Dev Org",
    "edition": "Developer",
    "features": ["OmniStudioDesigner", "OmniStudioRuntime"],
    "settings": {
        "lightningExperienceSettings": {
            "enableS1DesktopEnabled": true
        },
